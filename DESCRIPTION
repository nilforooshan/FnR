Package: FnR
Title: Inbreeding and Numerator Relationship Coefficients
Version: 1.1.0
Authors@R: 
    person("Mohammad Ali", "Nilforooshan", , "m.a.nilforooshan@gmail.com", role = c("aut", "cre"),
